PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX fideo: <http://purl.obolibrary.org/obo/fideo/>
PREFIX IAO: <http://purl.obolibrary.org/obo/IAO_>

SELECT DISTINCT ?ID ?property ?filler
WHERE {
  ?ID rdfs:subClassOf fideo:FIDEO_00000005 .
  ?ID rdfs:subClassOf ?anonUnion .
  ?anonUnion owl:unionOf/rdf:rest*/rdf:first ?restriction .
  ?restriction owl:onProperty ?property ;
               owl:someValuesFrom ?filler .
}